{
    "name": "kodekeep/sushi-countries",
    "description": ":package_description",
    "keywords": [
        "kodekeep",
        "sushi-countries"
    ],
    "homepage": "https://github.com/kodekeep/sushi-countries",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "brian@kodekeep.com",
            "homepage": "https://kodekeep.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "calebporzio/sushi": "^2.1",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "kodekeep/mobile-codes": "^1.0",
        "mledoze/countries": "^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "graham-campbell/analyzer": "^2.4",
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.3",
        "symfony/var-dumper": "4.4"
    },
    "autoload": {
        "psr-4": {
            "KodeKeep\\SushiCountries\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KodeKeep\\SushiCountries\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:coverage": "vendor/bin/phpunit --coverage-html=.coverage --coverage-clover=clover.xml",
        "format": "vendor/bin/php-cs-fixer fix ./"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
